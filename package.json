{
  "name": "meal-management",
  "version": "0.1.0",
  "description": "Plan, create and manage the meals in your life",
  "repository": "git@github.com:reubenberghan/meal-management.git",
  "author": "Reuben Berghan <reuben.berghan@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "connected-react-router": "^5.0.1",
    "history": "^4.7.2",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.11.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "styled-components": "^4.0.3",
    "styled-normalize": "^8.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.1.3",
    "jest-enzyme": "^7.0.1",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.0.4",
    "prettier-standard": "^8.0.1",
    "react-test-renderer": "^16.6.1",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/node_modules/",
      "!src/constants.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/state/constants.js",
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
